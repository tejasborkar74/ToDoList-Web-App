<%- include("header") -%>

    <div class="box" , id="heading">
      <h1><%= listTitle %></h1>
    </div>

    <div class="box">
      <ul>
        <%for(var i=0;i<listArray.length;i++){%>

          <div class="item">
            <input type="checkbox">
            <p><%= listArray[i] %></p>
          </div>

        <%}%>
      </ul>

      <form class="item" action="/" method="post">

        <input type="text" name="newItem" value="" placeholder="New Item" autocomplete="off">
        <button type="submit" name="button" value = <%= listTitle %> >+</button>

      </form>
    </div>
<%- include("footer") -%>
